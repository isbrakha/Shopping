<%- include('../partials/header') %>

<h2>List of Products</h2>
  <thead>
    <tr>
      <th>PRODUCT NAME</th>
    </tr>
  </thead><br>
  <tbody>
    <% products.forEach(p => { %>
      <tr>
        <td><%= p.name %></td>
        <td><a href="/products/<%= p._id %>">DETAILS</a></td><br>
      </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>